{
    "name": "arthur/alphavel",
    "description": "Ultra-fast PHP framework with Swoole and Laravel-style features",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "psr/container": "^2.0",
        "psr/log": "^3.0",
        "vlucas/phpdotenv": "^5.0",
        "alphavel/alphavel": "^2.0",
        "alphavel/cache": "^2.0",
        "alphavel/database": "^2.0",
        "alphavel/events": "^2.0",
        "alphavel/logging": "^2.0",
        "alphavel/validation": "^2.0",
        "alphavel/support": "^2.0"
    },
    "require-dev": {
        "laravel/pint": "^1.25",
        "phpunit/phpunit": "^10.5",
        "squizlabs/php_codesniffer": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "storage/framework/facades.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "alphavel": {
            "providers": [
                "Alphavel\\Cache\\CacheServiceProvider",
                "Alphavel\\Database\\DatabaseServiceProvider",
                "Alphavel\\Events\\EventServiceProvider",
                "Alphavel\\Logging\\LoggingServiceProvider",
                "Alphavel\\Validation\\ValidationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
